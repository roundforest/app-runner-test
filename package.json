{
  "name": "app-runner-remix",
  "version": "1.1.16",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -m",
    "build:app": "remix build",
    "postbuild": "npm run docker -- --load",
    "publish": "concurrently -r 'npm:publish:*'",
    "publish:docker": "if [[ $(uname -a) =~ 'X86' ]]; then docker push ${REMIX_APP_RUNNER_TEST_ECR_URI}:${npm_package_version}; fi",
    "publish:docker-multiarch": "if [[ $(uname -a) =~ 'ARM64' ]]; then npm run docker -- --platform linux/amd64,linux/arm64 --push; fi",
    "docker": "docker buildx build -t ${REMIX_APP_RUNNER_TEST_ECR_URI}:${npm_package_version} --secret id=npmrc,src=$(npm get userconfig) --build-arg NODE_VERSION=${npm_package_config_docker_node_version} .",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -w",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/serve": "^1.18.1",
    "@roundforest/functional-commons": "^1.7.25",
    "@tailwindcss/forms": "^0.5.4",
    "@types/set-cookie-parser": "^2.4.3",
    "crypto-random-string": "^5.0.0",
    "env-var": "^7.3.1",
    "isbot": "^3.6.12",
    "launchdarkly-node-server-sdk": "^7.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "set-cookie-parser": "^2.6.0",
    "type-fest": "^4.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
