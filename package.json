{
  "name": "app-runner-remix",
  "version": "1.1.1",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -m",
    "build:app": "remix build",
    "postbuild": "npm run docker -- --load",
    "setup": "npm i && npm run build",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css -w",
    "postpublish": "concurrently -r 'npm:publish:*'",
    "publish:docker": "if [[ $(uname -a) =~ 'X86' ]]; then docker push ${REMIX_APP_RUNNER_TEST_ECR_URI}:${npm_package_version}; fi",
    "publish:docker-multiarch": "if [[ $(uname -a) =~ 'ARM64' ]]; then npm run docker -- --platform linux/amd64,linux/arm64 --push; fi",
    "docker": "docker buildx build -t ${REMIX_APP_RUNNER_TEST_ECR_URI}:${npm_package_version} --secret id=npmrc,src=$(npm get userconfig) --build-arg NODE_VERSION=${npm_package_config_docker_node_version} .",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@tailwindcss/forms": "^0.5.3",
    "isbot": "^3.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "type-fest": "^3.12.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
